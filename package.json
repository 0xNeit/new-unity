{
  "name": "new-unity",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-version-file": "genversion --es6 --semi src/constants/version.ts",
    "generate-contract-types": "rimraf ./src/types/contracts && npm-run-all generate-contract-types:app generate-contract-types:contract-package",
    "generate-contract-types:app": "typechain --target=ethers-v5 --out-dir=src/types/contracts 'src/constants/contracts/abis/*.json'",
    "generate-contract-types:contract-package": "typechain --target=ethers-v5 --out-dir=./src/packages/contracts/types/contracts/others './src/packages/contracts/contractInfos/externalAbis/*.json' && typechain --target=ethers-v5 --out-dir=./src/packages/contracts/types/contracts/unityProtocol './node_modules/unity-core-contracts/artifacts/contracts/!(Factories)/**/*[!.dbg].json' && typechain --target=ethers-v5 --out-dir=./src/packages/contracts/types/contracts/ucoreVesting './node_modules/unity-core-contracts/artifacts/contracts/Tokens/UCORE/UCOREVesting.sol/UCOREVesting.json'",
    "postinstall": "npm-run-all --parallel generate-contract-types generate-version-file"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@infinitywallet/infinity-connector": "^1.0.5",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@mui/material": "^5.14.4",
    "@pancakeswap/sdk": "^5.2.1",
    "@sentry/react": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@types/comma-number": "^2.1.0",
    "@uiw/react-markdown-preview": "^4.1.13",
    "@uiw/react-md-editor": "^3.14.3",
    "bignumber.js": "^9.0.0",
    "buffer": "^6.0.3",
    "comma-number": "^2.1.0",
    "copy-to-clipboard": "^3.3.1",
    "core-multicall": "2.15.0",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "i18next": "^21.6.15",
    "lodash": "^4.17.21",
    "noop-ts": "^1.0.6",
    "posthog-js": "^1.57.2",
    "prop-types": "15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.16.5",
    "react-number-format": "^4.4.1",
    "react-query": "^3.34.16",
    "react-router-dom": "^5.0.1",
    "react-toastify": "9.0.1",
    "recompose": "^0.26.0",
    "rehype-sanitize": "^5.0.1",
    "sass": "^1.65.1",
    "styled-components": "^6.0.7",
    "unity-core-contracts": "^0.2.0",
    "wagmi": "^0.12.19",
    "web3": "1.3.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@typechain/ethers-v5": "^11.1.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/recompose": "^0.30.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "fs": "^0.0.1-security",
    "genversion": "^3.1.1",
    "npm": "^9.6.5",
    "npm-run-all2": "^6.0.5",
    "rimraf": "^4.1.2",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^5.1.0",
    "typechain": "^8.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.3",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  }
}
